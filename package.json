    "start:meetings": "cd ../Meetings/ && npm run start",
    "start:landing": "cd ../Landing/ && npm run start",
    "start:layouts": "cd ../Layouts/ && npm run start",
    "start:messages": "cd ../Messages/ && npm run start",
    "start:profile": "cd ../Profile/ && npm run start",
    "start:search": "cd ../Search/ && npm run start",
    "start:all": "npm run kill-all-ports && run-p start start:auth start:meetings start:landing start:layouts start:messages start:search start:profile",
