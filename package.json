    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    ".(js, jsx, ts, tsx)": [
      "tslint",
      "prettier --write"
    ],
    ".(json, css, md)": [
      "prettier â€”write"
    ]
  }
}
