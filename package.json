    "clone:profile": "git clone https://github.com/arenchilingaryan/photoGate-Profile.git ../Profile",
    "clone:search": "git clone https://github.com/arenchilingaryan/photoGate-Search.git ../Search",
    "clone:designsystem": "git clone https://github.com/arenchilingaryan/photoGate-design-system.git ../Design-System",
    "install:auth": "cd ../Auth/ && npm install",
    "install:meetings": "cd ../Meetings/ && npm install",
    "install:landing": "cd ../Landing/ && npm install",
    "install:layouts": "cd ../Layouts/ && npm install",
    "install:messages": "cd ../Messages/ && npm install",
    "install:profile": "cd ../Profile/ && npm install",
    "install:search": "cd ../Search/ && npm install",
    "install:designsystem": "cd ../Design-System/ && npm install",
    "install:all": "npm install && npm run install:auth && npm run install:meetings && npm run install:landing && npm run install:layouts && npm run install:messages && npm run install:profile && npm run install:search",
    "createfolders": "cd .. && mkdir Auth && mkdir Meetings && mkdir Landing && mkdir Layouts && mkdir Messages && mkdir Profile && mkdir Search && mkdir Design-System",
    "clone:all": "npm run clone:auth && npm run clone:meetings && npm run clone:landing && npm run clone:layouts && npm run clone:messages && npm run clone:profile && npm run clone:search",
    "setup:all": "npm run createfolders && npm run clone:all && npm run install:all",
    "pull:auth": "cd ../Auth && git pull",
    "pull:meetings": "cd ../Meetings && git pull",
    "pull:landing": "cd ../Landing && git pull",
    "pull:layouts": "cd ../Layouts && git pull",
    "pull:messages": "cd ../Messages && git pull",
    "pull:profile": "cd ../Profile && git pull",
    "pull:search": "cd ../Search && git pull",
    "pull:all": "npm run pull:auth && npm run pull:meetings && npm run pull:landing && npm run pull:layouts && npm run pull:messages && npm run pull:profile && npm run pull:search && git pull"
  },
